{
  "name":"webforge/console",
  "description": "Abstraction layer around the symfony console to write tested commands with ease",
  "license":"MIT",
  "authors":[
    {
      "name":"Philipp Scheit",
      "email":"p.scheit@ps-webforge.com"
    }
  ],
  "minimum-stability":"dev",
  "require":{
    "symfony/console": "2.3.*@stable"
  },
  "require-dev": {
    "webforge/testplate": "1.*",
    "mockery/mockery": "dev-master"
  },
  "autoload":{
    "psr-0":{
      "Webforge\\Console":["lib/", "tests/"]
    }
  },
  "extra":{
    "branch-alias":{
      "dev-master":"1.0.x-dev"
    }
  }
}
